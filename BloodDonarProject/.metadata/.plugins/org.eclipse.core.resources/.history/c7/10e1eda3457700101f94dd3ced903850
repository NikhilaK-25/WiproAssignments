package com.nk.wipro.repo;


import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.nk.wipro.entity.Donor;
import com.nk.wipro.enums.BloodGroup;

import java.util.List;

@Repository
public interface DonorRepository extends JpaRepository<Donor, Long> {
	List<Donor> findByBloodGroup(BloodGroup bloodGroup);

    List<Donor> findByCity(String city);
    List<Donor> findByCompany(String company);
}
